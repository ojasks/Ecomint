#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/goldbook/Desktop/Ecomint Extension/example-plasmo/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.12.9_@swc+helpers@0.5.17__@swc+helpers@0.5.17_@types+node@20._bf2fdb6e8bbd96ae7314b87dde3e9a0c/node_modules/plasmo/bin/node_modules:/Users/goldbook/Desktop/Ecomint Extension/example-plasmo/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.12.9_@swc+helpers@0.5.17__@swc+helpers@0.5.17_@types+node@20._bf2fdb6e8bbd96ae7314b87dde3e9a0c/node_modules/plasmo/node_modules:/Users/goldbook/Desktop/Ecomint Extension/example-plasmo/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.12.9_@swc+helpers@0.5.17__@swc+helpers@0.5.17_@types+node@20._bf2fdb6e8bbd96ae7314b87dde3e9a0c/node_modules:/Users/goldbook/Desktop/Ecomint Extension/example-plasmo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/goldbook/Desktop/Ecomint Extension/example-plasmo/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.12.9_@swc+helpers@0.5.17__@swc+helpers@0.5.17_@types+node@20._bf2fdb6e8bbd96ae7314b87dde3e9a0c/node_modules/plasmo/bin/node_modules:/Users/goldbook/Desktop/Ecomint Extension/example-plasmo/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.12.9_@swc+helpers@0.5.17__@swc+helpers@0.5.17_@types+node@20._bf2fdb6e8bbd96ae7314b87dde3e9a0c/node_modules/plasmo/node_modules:/Users/goldbook/Desktop/Ecomint Extension/example-plasmo/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.12.9_@swc+helpers@0.5.17__@swc+helpers@0.5.17_@types+node@20._bf2fdb6e8bbd96ae7314b87dde3e9a0c/node_modules:/Users/goldbook/Desktop/Ecomint Extension/example-plasmo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../plasmo/bin/index.mjs" "$@"
else
  exec node  "$basedir/../plasmo/bin/index.mjs" "$@"
fi
